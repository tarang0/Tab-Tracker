{
  "manifest_version": 3,
  "name": "TabTracker",
  "version": "1.0.0",
  "description": "Captures screen time from active pages",
  "permissions": [
    "tabs",
    "storage",
    "idle"
  ],
  "key":"",
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "images/icon16.png"
    }
  },
  "icons": {
    "16": "images/icon16.png"
  }
}
